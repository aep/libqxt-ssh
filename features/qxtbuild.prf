!win32:VERSION = 0.3
incs.path      = $${include.path}/$${CLEAN_TARGET}/

contains(CONVENIENCE, $${CLEAN_TARGET}){
    for(header,HEADERS){
        incs.files += $$header
    }
    incs.files += ../../deploy/include/$${CLEAN_TARGET}/*
}

LIBS += -L../../deploy/libs/

INSTALLS = target incs
include(../../deploy/qt/qxt.prf)
